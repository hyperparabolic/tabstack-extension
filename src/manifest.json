{
    "applications": {
      "gecko": {
        "strict_min_version": "58.0a1"
      }
    },
    "background": {
      "scripts": ["background/index.js"]
    },
    "browser_action": {
      "browser_style": true,
      "default_icon": {
        "48": "icon.png",
        "96": "icon96.png"
      },
      "default_title": "Tabstack",
      "default_popup": "popup/index.html"
    },
    "description": "Stack based temporary tab storage.",
    "manifest_version": 2,
    "name": "Tabstack",
    "permissions": [
      "tabs"
    ],
    "version": "0.1"
}
